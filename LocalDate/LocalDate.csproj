<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <RootNamespace>LocalDate</RootNamespace>
    <PackOnBuild>true</PackOnBuild>
    <PackageProjectUrl>https://github.com/amir734jj/LocalDate.git</PackageProjectUrl>
    <Title>Date only library, similar to C# native DateTime but only Date with no time. This library uses Julian Day number for date calculations. This library is intended to be simple to use solution to lack of Date only type in C#.</Title>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MongoDB.Driver" Version="2.6.1" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>
</Project>